{
    "name": "tomaszgasior/create-symfony-project",
    "description": "Composer plugin providing command for creating new projects based on Symfony framework.",
    "type": "composer-plugin",
    "license": "MIT",
    "authors": [{
        "name": "Tomasz GÄ…sior",
        "homepage": "https://tomaszgasior.pl"
    }],
    "require": {
        "composer-plugin-api": "^2.0",
        "symfony/http-client": "<=4.4.26 || <=5.3.3"
    },
    "require-dev": {
        "composer/composer": "^2.0",
        "symfony/console": "^4.0",
        "symfony/var-dumper": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Mushrooms\\CreateSymfonyProject\\": "src/"
        }
    },
    "extra": {
        "class": "Mushrooms\\CreateSymfonyProject\\Composer\\Plugin"
    },
    "config": {
        "sort-packages": true
    }
}
